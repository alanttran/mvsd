{
  "name": "mvsd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:github": "node scripts/generate-sitemap.js github && node scripts/generate-robots.js github && cross-env NODE_ENV=production VITE_DEPLOY_TARGET=github vite build",
    "build:production": "node scripts/generate-sitemap.js production && node scripts/generate-robots.js production && cross-env NODE_ENV=production VITE_DEPLOY_TARGET=production vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "preview:github": "cross-env VITE_DEPLOY_TARGET=github vite preview",
    "preview:production": "cross-env VITE_DEPLOY_TARGET=production vite preview",
    "sitemap:github": "node scripts/generate-sitemap.js github",
    "sitemap:production": "node scripts/generate-sitemap.js production",
    "robots:github": "node scripts/generate-robots.js github",
    "robots:production": "node scripts/generate-robots.js production"
  },
  "dependencies": {
    "reset-css": "^5.0.2",
    "vue": "^3.4.29",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@vheemstra/vite-plugin-imagemin": "^2.2.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "cross-env": "^7.0.3",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "sass": "^1.91.0",
    "vite": "^5.3.3"
  }
}
